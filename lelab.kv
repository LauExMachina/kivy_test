    Screen:
        orientation: "vertical"
        name: "MenuPrincipal"
        on_enter: menu_principal.on_enter_once()
        MenuPrincipal:
            id: menu_principal


# --------------------------------------------- Screen Sound -----------------------------------------------------------------

    Screen:
        name: "SoundsLab"               # Identifiant pour le menu principal
        BoxLayoutWithActionBar:
            title: "Menu"
            SoundsLab:                  # Contenu de la page


# ---------------------------------------------  Screen Pr√©sentation -----------------------------------------------------------------
    Screen:
        name: "PresentationTab"
        BoxLayoutWithActionBar:
            title: "Menu"
            PresentationTab:


