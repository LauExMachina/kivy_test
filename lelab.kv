    Screen:
        name: "LayoutExemplesTabs"
        BoxLayoutWithActionBar:
            title: "Menu"
            LayoutExemplesTabs:

# -------------------------------------------- Screen Widgets ----------------------------------------------------------

    Screen:
        name: "WidgetExemplesTabs"
        BoxLayoutWithActionBar:
            title: "Menu"
            WidgetExemplesTabs:

# ----------------------------------------------------- Screen Canvas ------------------------------------------------------------

    Screen:
        name: "CanvasExemplesTabs"
        BoxLayoutWithActionBar:
            title: "Menu"
            CanvasExemplesTabs:

